#!/bin/sh
host=192.168.10.40
string='application/x-rtp, media=(string)audio, clock-rate=(int)24000, encoding-name=(string)VORBIS, encoding-params=(string)1, configuration=(string), payload=(int)96'

#string='application/x-rtp, media=(string)audio, clock-rate=(int)24000, encoding-name=(string)VORBIS, encoding-params=(string)1, configuration=(string), payload=(int)96'

gst-launch -v tcpclientsrc port=5000 host=$host ! \
	$string		!\
	rtpvorbisdepay	!\
	 vorbisdec	!\
	 audioconvert	!\
	 autoaudiosink

